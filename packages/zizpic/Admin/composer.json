{
    "name": "zizpic/admin",
    "description": "zizpic management using Laravel 4 / 5",
    "keywords": ["laravel", "zizpic", "zizpic-management"],
    "authors": [
        {
            "name": "amit",
            "email": "amit.n@cisinlabs.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": "4.* | 5.*",
        "illuminate/support": "4.* | 5.*",
        "baum/baum": "1.0.* | 1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Modules\\Zizpic\\": "src/"
        }
    },
    "autoload-dev":
            {
                "psr-4": {
                    "Modules\\Zizpic\\Tests\\": "tests/"
                }
            },
    "minimum-stability": "stable"
}
